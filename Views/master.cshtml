    @RenderSection("Header", required: false)

</head>


<body class="frontpage theme-font-ariel theme-color-black">


    <header class="header">





        @Html.Partial("~/Views/Partials/Navigation/TopNavigation.cshtml")


        <div class="mobile-nav-handler">

            <div class="hamburger lines" id="toggle-nav">

                <span></span>

            </div>

        </div>


    </header>


    <main>

        @RenderBody()

    </main>


    <footer class="section--themed">

        <div class="container">

            <div class="row">

                <div class="col-md-12 ta-center fixed-bottom">

                    @home.FooterAddress

